	Algoritmo Ejercicio09Operaciones
		Definir a, b Como Real
		Escribir "Ingrese dos numeros:"
		Leer a, b
		Escribir "Suma: ", a + b
		Escribir "Resta: ", a - b
		Escribir "Multiplicacion: ", a * b
		
		Si b <> 0 Entonces
			Escribir "Division: ", a / b
		SiNo
			Escribir "No se puede dividir entre cero"
		FinSi


FinAlgoritmo
